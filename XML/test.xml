<?xml version="1.0" encoding="UTF-8"?>

<suite name="test"
 
	user="danrusu"
	 
	project="test">


	<test name="test" browser="chrome">

	
		<testcase name="projects.common.testCases.OpenPage" 
		
			url="http://danrusu.ro/test.html" />


 		<testcase name="projects.common.testCases.WebElementScreenShot"
		
			scaling="1.25" 
		
		 	selector="cssselector:body"
		 	
		 	file="body.png" />	
			

		<testcase name="projects.common.testCases.WebElementScreenShot" 
		
			scaling="1.25"
		
		 	selector="cssselector:.flexbox-header"
		 	
		 	file="flexboxHeader.png" />	


		<testcase name="projects.common.testCases.WebElementScreenShot"
		
			scaling="1.25" 
		
		 	selector="cssselector:.grid-page"
		 	
		 	file="gridPage.png" />	
		 		 
		 	
		 	
		<testcase name="projects.common.testCases.WebElementScreenShotCompare"
		
			expectedFailureRegExp=".*Screenshots are different.*"
		
			scaling="1.25" 
		 	
		 	selector1="id:slot_machine_master"
		 	
		 	selector2="id:slot_machine"
		 	
		 	
		 	file1="slot_machine_master.png" 
		 	
		 	file2="slot_machine.png"
		 	
		 	diffFile="diff.png" 
		 	
		 	stopOnFailure="false"/>
		 	
		 
		<testcase name="projects.common.testCases.WebElementScreenShotCompare"
		
		    note="slot_machine_master2.png == slot_machine_master3.png == noDiff.png"
		
			scaling="1.25" 
		 	
		 	selector1="id:slot_machine_master"
		 	
		 	selector2="id:slot_machine_master"
		 	
		 	
		 	file1="slot_machine_master2.png" 
		 	
		 	file2="slot_machine_master3.png"
		 	
		 	diffFile="noDiff.png" />	
		 				 				 	

	</test>
	
	
</suite>